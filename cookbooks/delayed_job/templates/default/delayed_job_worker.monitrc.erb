check process <%= @worker_name %>
  with pidfile /data/<%= @app_name %>/shared/pids/<%= @worker_name %>.pid
  start program = "RAILS_ENV=production /data/<%= @app_name %>/current/script/delayed_job start"
  stop program = "RAILS_ENV=production /data/<%= @app_name %>/current/script/delayed_job stop"
  group <%= @app_name %>_jobs