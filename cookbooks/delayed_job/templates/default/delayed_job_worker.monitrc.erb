check process <%= @worker_name %>
  with pidfile /data/<%= @app_name %>/shared/pids/<%= @worker_name %>.pid
  start program = "/data/<%= @app_name %>/current/script/delayed_job -e <%= @framework_env %> start"
  stop program = "/data/<%= @app_name %>/current/script/delayed_job -e <%= @framework_env %> stop"
  group <%= @app_name %>_jobs